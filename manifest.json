{
  "name": "SVG Replacer",
  "version": "0.1",
  "description": "Replaces SVG with custom ones on websites",
  "author": "Eason Wong",
  "browser_specific_settings": {
    "gecko": {
      "id": "SVGR@EasonWong"
    }
  },
  "permissions": [
    "activeTab",
    "tabs",
    "theme",
    "storage"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "replace_svg.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "browser_style": true
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_title": "SVG Replacer",
    "browser_style": true,
    "default_icon": {
      "16": "images/ATBC_16.png",
      "32": "images/ATBC_32.png",
      "48": "images/ATBC_48.png",
      "96": "images/ATBC_96.png",
      "128": "images/ATBC_128.png"
    }
  },
  "icons": {
    "16": "images/ATBC_16.png",
    "32": "images/ATBC_32.png",
    "48": "images/ATBC_48.png",
    "96": "images/ATBC_96.png",
    "128": "images/ATBC_128.png"
  },
  "manifest_version": 2
}